    "mini-css-extract-plugin": "0.9.0",
    "postcss": "7.0.27",
    "postcss-loader": "3.0.0",
    "stats-webpack-plugin": "0.7.0",
    "stylelint": "13.2.1",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-standard": "20.0.0",
    "terser-webpack-plugin": "2.3.5",
    "webpack": "4.42.1",
